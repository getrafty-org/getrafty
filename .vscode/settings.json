{
  "cmake.debugger": "lldb-dap",
  "cmake.debugConfig": {
    "type": "lldb-dap",
    "request": "launch",
    "console": "integratedTerminal",
    "env": {
      "UBSAN_OPTIONS": "print_stacktrace=1"
    },
    "stopOnEntry": false,
    "debuggerPath": "lldb-dap"
  },
  "cmake.generator": "Unix Makefiles",
  "clangd.path": "clangd-21",
  "clangd.arguments": [
    "--clang-tidy",
  ],
  "cSpell.words": [
    "bugprone",
    "cppcoreguidelines",
    "epoll",
    "EPOLLERR",
    "EPOLLHUP",
    "EPOLLIN",
    "EPOLLOUT",
    "EPOLLRDHUP",
    "fgmpack",
    "getrafty",
    "gtest",
    "MPMC",
    "MPSC",
    "Vyukov"
  ]
}